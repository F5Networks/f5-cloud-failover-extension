{
    "name": "f5-cloud-failover",
    "version": "0.9.0",
    "author": "F5 Networks",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/F5Networks/f5-cloud-failover.git"
    },
    "scripts": {
        "install-production": "npm install --production --no-optional",
        "lint": "eslint src test",
        "test-only": "mocha --recursive \"./test/unittests/*.js\" --opts ./test/unittests/.mocha.opts",
        "test": "nyc npm run test-only",
        "check": "nyc check-coverage",
        "report": "nyc report --reporter=html"
    },
    "nyc": {
        "check-coverage": true,
        "lines": 70,
        "functions": 70,
        "branches": 70,
        "reporter": [
            "html",
            "text"
        ]
    },
    "dependencies": {
        "@f5devcentral/f5-cloud-libs": "beta",
        "@google-cloud/compute": "^1.0.1",
        "@google-cloud/storage": "^2.5.0",
        "ajv": "^6.10.0",
        "ajv-async": "^1.0.1",
        "ajv-cli": "^3.0.0",
        "aws-sdk": "^2.369.0",
        "azure-arm-network": "5.1.0",
        "azure-storage": "^2.8.1",
        "commander": "^2.19.0",
        "gtoken": "3.0.0",
        "ms-rest-azure": "^2.6.0",
        "request": "^2.83.0",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@f5devcentral/eslint-config-f5-atg": "^0.1.1",
        "acorn": "^6.0.0",
        "eslint": "^5.16.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.17.3",
        "icrdk": "git://github.com/f5devcentral/f5-icontrollx-dev-kit#master",
        "mocha": "^5.2.0",
        "nyc": "^14.0.0",
        "ssh2": "^0.8.2"
    },
    "optionalDependencies": {
        "mermaid.cli": "^0.5.1"
    },
    "eslintConfig": {
        "extends": "@f5devcentral/eslint-config-f5-atg",
        "rules": {
            "func-names": "off"
        }
    }
}
