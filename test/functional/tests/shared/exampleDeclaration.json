{
    "class": "Cloud_Failover",
    "environment": "{{environment}}",
    "storageResource": "mystorageaccount",
    "storageTags": [
      {
        "key": "F5_CLOUD_FAILOVER_STORAGE_LABEL",
        "value": "{{deploymentId}}"
      }
    ],
    "failoverRoutes": {
      "scopingTags": {
        "F5_CLOUD_FAILOVER_LABEL": "{{deploymentId}}"
      },
      "scopingAddressRanges": [
        "192.0.2.0/24"
      ]
    },
    "addressTags": [
      {
        "key": "F5_CLOUD_FAILOVER_LABEL",
        "value": "{{deploymentId}}"
      }
    ]
}