{
    "class": "Cloud_Failover",
    "environment": "{{environment}}",
    "controls": {
      "class": "Controls",
      "logLevel": "info"
    },
    "externalStorage": {
      "scopingTags": {
        "f5_cloud_failover_label": "{{deploymentId}}"
      }
    },
    "failoverAddresses": {
      "scopingTags": {
        "f5_cloud_failover_label": "{{deploymentId}}"
      }
    },
    "failoverRoutes": {
      "scopingTags": {
        "f5_cloud_failover_label": "{{deploymentId}}"
      },
      "scopingAddressRanges": [
        {
          "range": "192.0.2.0/24",
          "nextHopAddresses": {
            "discoveryType": "static",
            "items": [
              "{{nextHopAddress1}}",
              "{{nextHopAddress2}}"
            ]
          }
        },
        {
          "range": "2001:db8:2:2::/64",
          "nextHopAddresses": {
            "discoveryType": "static",
            "items": [
              "{{nextHopAddress1_IPv6}}",
              "{{nextHopAddress2_IPv6}}"
            ]
          }
        },
        {
          "range": "192.0.3.0/24",
          "nextHopAddresses": {
            "discoveryType": "static",
            "items": [
              "{{nextHopAddress1}}",
              "{{nextHopAddress2}}"
            ]
          }
        }
      ],
      "defaultNextHopAddresses": {
        "discoveryType": "static",
        "items": [
          "{{nextHopAddress1}}",
          "{{nextHopAddress2}}",
          "{{nextHopAddress1_IPv6}}",
          "{{nextHopAddress2_IPv6}}"
        ]
      }
    }
}
