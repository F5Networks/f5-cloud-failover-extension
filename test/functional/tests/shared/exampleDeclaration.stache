{
    "class": "Cloud_Failover",
    "environment": "{{environment}}",
    "controls": {
      "class": "Controls",
      "logLevel": "info"
    },
    "externalStorage": {
      "scopingTags": {
        "f5_cloud_failover_label": "{{deploymentId}}"
      }
    },
    "failoverAddresses": {
      "enabled": true,
      "scopingTags": {
        "f5_cloud_failover_label": "{{deploymentId}}"
      }
    },
    "failoverRoutes": {
      "enabled": true,
      "routeGroupDefinitions": [{
          "scopingTags": {
            "f5_cloud_failover_label": "{{deploymentId}}"
          },
          "scopingAddressRanges": [
            {{#scopingAddressRanges}}
            {
              "range": "{{range}}"
            }{{^last}},{{/last}}
            {{/scopingAddressRanges}}
          ],
          "defaultNextHopAddresses": {
            "discoveryType": "static",
            "items": [
              {{#nextHopAddresses}}
              "{{address}}"{{^last}},{{/last}}
              {{/nextHopAddresses}}
            ]
          }
        }]
    }

}
