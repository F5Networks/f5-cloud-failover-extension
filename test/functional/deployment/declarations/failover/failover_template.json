{
	"class": "Cloud_Failover",
	"environment": "${enviroment}",
	"storageResource": "${storage_resource}",
	"storageTags": [
		{
			"key": "${storage_key}",
			"value": "${storage_value}"
		}
	],
	"failoverRoutes": {
		"scopingTags": {
			"${tag_name}": "${tag_value}"
		},
		"scopingAddressRanges":[
			"${managed_routes}"
		]
	},
	"addressTags": [
		{
			"key": "${tag_name}",
			"value": "${tag_value}"
		}
	]
}