{
    "class":"Cloud_Failover",
    "environment":"aws",
    "externalStorage":{
       "scopingTags":{
          "f5_cloud_failover_label":"mydeployment"
       }
    },
    "failoverAddresses":{
       "enabled":true,
       "scopingTags":{
          "f5_cloud_failover_label":"mydeployment"
       }
    },
    "failoverRoutes":{
       "enabled":true,
       "scopingTags":{
          "f5_cloud_failover_label":"mydeployment"
       },
       "scopingAddressRanges":[
          {
             "range":"192.168.11.0/24",
             "nextHopAddresses":{
                "discoveryType":"routeTag"
             }
          },
          {
             "range":"192.168.12.0/24",
             "nextHopAddresses":{
                "discoveryType":"routeTag"
             }
          }
       ]
    }
 }