{
    "class": "Cloud_Failover",
    "environment": "aws",
    "storageResource": "myuniquestorageaccount",
    "storageTags": [
		{
			"key": "value",
			"value": "myvalue"
		}
	],
	"failoverRoutes": {
		"scopingTags": {
			"F5_CLOUD_FAILOVER_LABEL": "mydeployment"
		},
		"scopingAddressRanges":[
			"192.168.1.0/24"
		]
	},
	"addressTags": [
		{
			"key": "F5_CLOUD_FAILOVER_LABEL",
			"value": "mydeployment"
		}
	]
}
