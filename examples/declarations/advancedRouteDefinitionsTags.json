{
    "class": "Cloud_Failover",
    "environment": "aws",
    "externalStorage": {
      "scopingTags": {
        "f5_cloud_failover_label": "mydeployment"
      }
    },
    "failoverAddresses": {
      "enabled": true,
      "scopingTags": {
        "f5_cloud_failover_label": "mydeployment"
      }
    },
    "failoverRoutes": {
      "enabled": true,
      "routeGroupDefinitions": [
        {
          "scopingTags": {
            "f5_cloud_failover_label": "route-group-1"
          },
          "scopingAddressRanges": [
            {
              "range": "192.168.1.0/24",
              "nextHopAddresses": {
                "discoveryType": "static",
                "items": [
                  "192.0.2.12",
                  "192.0.2.13"
                ]
            }
            },
            {
              "range": "192.168.1.1/24"
            }
          ],
          "defaultNextHopAddresses": {
            "discoveryType": "static",
            "items": [
                "192.0.2.10",
                "192.0.2.11"
            ]
          }
        },
        {
          "scopingTags": {
            "f5_cloud_failover_label": "route-group-2"
          },
          "scopingAddressRanges": [
            {
              "range": "0.0.0.0/0"
            }
          ],
          "defaultNextHopAddresses": {
            "discoveryType": "static",
            "items": [
                "192.0.2.10",
                "192.0.2.11"
            ]
          }
        }
      ]
    }
}
