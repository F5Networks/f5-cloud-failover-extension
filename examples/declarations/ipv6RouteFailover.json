              },
              {
                 "range":"2001:db8:2:2::/64"
              }
           ],
           "defaultNextHopAddresses":{
              "discoveryType":"static",
              "items":[
                 "10.0.13.11",
                 "10.0.23.11",
                 "2600:1f13:12f:a803:5d15:e0e:1af9:8221",
                 "2600:1f13:12f:a804:5d15:e0e:1af9:8222"
              ]
           }
        }
     ]
  }
}