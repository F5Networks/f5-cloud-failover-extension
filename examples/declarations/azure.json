{
    "class": "Cloud_Failover",
    "environment": "azure",
	"externalStorage": {
		"scopingTags": {
			"f5_cloud_failover_label": "mydeployment"
		}
    },
	"failoverAddresses": {
		"enabled": true,
		"scopingTags": {
			"f5_cloud_failover_label": "mydeployment"
		}
    },
    "failoverRoutes": {
		"enabled": true,
		"scopingTags": {
			"f5_cloud_failover_label": "mydeployment"
		},
		"scopingAddressRanges": [
			{
				"range": "192.168.1.0/24"
			}
		],
		"defaultNextHopAddresses": {
			"discoveryType": "static",
			"items": [
				"192.0.2.10",
				"192.0.2.11"
			]
		}
    }
}
