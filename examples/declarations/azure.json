{
   "class":"Cloud_Failover",
   "environment":"azure",
   "controls":{
      "class":"Controls",
      "logLevel":"silly"
   },
   "externalStorage":{
      "scopingTags":{
         "f5_cloud_failover_label":"mydeployment"
      }
   },
   "failoverAddresses":{
      "enabled":true,
      "scopingTags":{
         "f5_cloud_failover_label":"mydeployment"
      }
   },
   "failoverRoutes":{
      "enabled":true,
      "routeGroupDefinitions":[
         {
            "scopingName":"myroutetable-1",
            "scopingAddressRanges":[
               {
                  "range":"0.0.0.0/0"
               }
            ],
            "defaultNextHopAddresses":{
               "discoveryType":"static",
               "items":[
                  "10.0.13.11",
                  "10.0.23.11"
               ]
            }
         }
      ]
   }
 }
 